{
  "manifest_version": 3,
  "name": "Super H",
  "version": "0.1",
  "description": "Experimental Superpower agent for the ChatGPT web interface",
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["src/content.js"],
      "css": ["src/content.css"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["storage"],
  "host_permissions": ["https://chat.openai.com/*"]
}
