{
  "manifest_version": 3,
  "name": "Super H",
  "version": "0.1",
  "description": "Experimental Superpower agent for the ChatGPT web interface",
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["src/audio.js", "src/content.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": ["microphone"]
}
